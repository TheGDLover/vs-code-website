ton><button class="game-button" onclick="openGame('https://replit.com/@leoht1130/my-sduhsd-net#game-website/FILE-games/1v1.LoL.html')">UNDERWHEELS</button><script>
document.getElementById("gameSearch").addEventListener("input", function() {
  let filter = this.value.toLowerCase();
  let buttons = document.querySelectorAll(".game-button");
  buttons.forEach(btn => {
    if (!["New Game(s)", "Delete Game", "Edit Game", "Download Website"].includes(btn.textContent)) {
      if (btn.textContent.toLowerCase().includes(filter)) {
        btn.style.display = "inline-block";
      } else {
        btn.style.display = "none";
      }
    }
  });
});
</script>
