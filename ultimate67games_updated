<script>
  window.addEventListener('DOMContentLoaded', function() {
    const allButtons = document.querySelectorAll('.game-button');
    const gameButtons = Array.from(allButtons).filter(btn => 
      !["New Game(s)", "Delete Game", "Edit Game", "Download Website"].includes(btn.textContent)
    );
    document.getElementById('gameCount').textContent = `Total Games: ${gameButtons.length}`;
  });
</script>
